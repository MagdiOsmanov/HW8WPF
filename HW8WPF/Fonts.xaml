<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:col ="clr-namespace:System.Collections;assembly=mscorlib"
                    xmlns:str="clr-namespace:System;assembly=mscorlib">
    <col:ArrayList x:Key="Fonts">
        <str:String>Arial</str:String>
        <str:String>Times New Roman</str:String>
        <str:String>Verdana</str:String>
    </col:ArrayList>
    <col:ArrayList x:Key="FontsSize">
        <str:String>12</str:String>
        <str:String>14</str:String>
        <str:String>16</str:String>
    </col:ArrayList>
</ResourceDictionary>